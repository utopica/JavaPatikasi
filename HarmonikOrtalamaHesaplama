
public class Main {
    static void harmonikOrtalama(int[] array){

        double harmonikToplam=0;

        for(double i=array[0];i<= array.length;i++){
            harmonikToplam += 1/i;
        }
        System.out.println("Harmonik Seri:" + harmonikToplam);

        System.out.println("Harmonik Ortalama: "+array.length/harmonikToplam);
    }
    

    public static void main(String[] args) {
        int[] list = {1,2,3,4,5};

       
        harmonikOrtalama(list);



    }
}
